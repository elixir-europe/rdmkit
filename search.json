---
layout: none
search: exclude
---

[
    {% for page in site.pages %}
    {% unless page.search == "exclude" %}
    {
        "title"    : "{{ page.title | escape }}",
        "keywords": "{{page.keywords | join: ', '}}",
        "summary": "{{page.summary | strip }}",
        "url"      : "{{ page.url }}"
    } 
    {% unless forloop.last and site.posts.size < 1 %},{% endunless %}
    {% endunless %}
    {% endfor %}
]
